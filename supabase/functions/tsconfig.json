{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "deno.ns"],
    "module": "esnext",
    "moduleResolution": "bundler",
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowJs": true,
    "jsx": "react-jsx"
  },
  "include": [
    "**/*.ts"
  ]
}

